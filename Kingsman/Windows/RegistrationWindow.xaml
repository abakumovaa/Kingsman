                    FontSize="22"
                    FontFamily="Krona One"
                    Width="200"
                    Height="50"
                    Margin="560 340 0 0"
                    Click="Show" Background="#B21A0B0B" Foreground="White"/>
        <StackPanel VerticalAlignment="Center">

            <TextBlock Text="REGISTRATION."
                       FontSize="55"
                       FontWeight="Bold"
                       HorizontalAlignment="Center"
                       FontFamily="Krona One"
                       Margin="-140 -20 0 0" Foreground="#FF54204E"/>

            <TextBlock Text="Login."
                       FontSize="18"
                       HorizontalAlignment="Center"
                       Margin="-500 30 0 0"
                       FontFamily="Krona One" Foreground="White"
                       />
            <TextBox Style="{DynamicResource TextBoxStyle3}" x:Name="TbLogin"
                     FontSize="18"                    
                     Width="200"
                     Height="30"
                     Margin="-500 8 0 0"/>

            <TextBlock Text="Password."
